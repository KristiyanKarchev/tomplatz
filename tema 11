Дайте пример за коректно именуване на компонентен тест, който проверява дали потребител получава грешка при опит за влизане с грешно потребителско име и парола

[Test]
public void Login_WithInvalidCredentials_ShouldReturnError()
{
    // Arrange
    var username = "invalid_username";
    var password = "invalid_password";

    // Act
    var result = LoginService.Login(username, password);

    // Assert
    Assert.IsTrue(result.IsError);
    Assert.AreEqual("Invalid credentials", result.ErrorMessage);
}

